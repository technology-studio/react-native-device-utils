{
  "name": "@txo/react-native-device-utils",
  "version": "4.0.5",
  "description": "Technology Studio - React native device utils",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/technology-studio/react-native-device-utils.git"
  },
  "author": {
    "name": "Technology Studio",
    "email": "npm@technologystudio.sk",
    "url": "https://github.com/technology-studio/"
  },
  "license": "UNLICENSED",
  "private": false,
  "scripts": {
    "build": "yarn build:clean && yarn build:lib",
    "build:clean": "yarn rimraf lib",
    "build:lib": "yarn tsc",
    "build:watch": "yarn tsc --watch",
    "test": "jest",
    "test:watch": "concurrently \"yarn build:watch\" \"jest --watch\"",
    "coverage": "jest --coverage && open coverage/lcov-report/index.html || xdg-open coverage/lcov-report/index.html",
    "compare-boilerplate-version": "./scripts/compare-boilerplate-version.sh",
    "lint": "eslint --format=node_modules/eslint-formatter-pretty --ext .jsx,.js,.ts,.tsx .",
    "fixcode": "eslint --format=node_modules/eslint-formatter-pretty --ext .jsx,.js,.ts,.tsx . --fix",
    "git-hook": "yarn -s lint-staged",
    "prepare": "husky install && yarn build",
    "print-txo-packages": "yarn list 2> /dev/null|grep @txo|sed 's/.*\\(@txo[^@]*\\)@^*\\([^ ]*\\).*/\\1@\\2/g'|sort|uniq",
    "sanity": "yarn lint && yarn build && tsc --noEmit && yarn test && yarn compare-boilerplate-version && echo 'success'",
    "semantic-release": "semantic-release",
    "update-boilerplate-version": "./scripts/update-boilerplate-version.sh"
  },
  "dependencies": {
    "ios-device-list": "^1.1.37",
    "uuid": "^9.0.0"
  },
  "peerDependencies": {
    "react-native": "*",
    "react-native-device-info": "^10.8.0",
    "react-native-keychain": "^8.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@txo-peer-dep/log": "^4.0.1",
    "@txo/log-console": "^3.0.0",
    "@types/jest": "^29.5.3",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/react-native": "^0.72.2",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "commitizen": "^4.3.0",
    "concurrently": "^8.2.0",
    "eslint": "^8.47.0",
    "eslint-config-txo-typescript-react": "^1.2.22",
    "eslint-formatter-pretty": "^5.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-redux-saga": "^1.3.2",
    "husky": "^8.0.3",
    "jest": "^29.6.2",
    "lint-staged": "^14.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.4",
    "react-native-device-info": "^10.8.0",
    "react-native-keychain": "^8.1.2",
    "redux-saga": "^1.2.3",
    "rimraf": "^5.0.1",
    "semantic-release": "^21.0.9",
    "semantic-release-slack-bot": "^4.0.2",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
